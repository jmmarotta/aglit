{
  "name": "aglit",
  "version": "0.1.4",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "aglit": "./dist/index.js"
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm --sourcemap --external @stricli/core --banner '#!/usr/bin/env bun\n' && tsgo -p tsconfig.json --emitDeclarationOnly",
    "prepack": "bun run build",
    "publish:dry": "bun publish --dry-run",
    "publish:run": "bun publish",
    "typecheck": "turbo run build --filter @jmmarotta/aglit-sdk && tsgo -p tsconfig.test.json --noEmit",
    "lint": "oxlint src",
    "fmt": "oxfmt --write src",
    "fmt:check": "oxfmt --check src",
    "test": "turbo run build --filter @jmmarotta/aglit-sdk && bun test"
  },
  "dependencies": {
    "@stricli/core": "^1"
  },
  "devDependencies": {
    "@jmmarotta/aglit-sdk": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmmarotta/agilt.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/jmmarotta/agilt/issues"
  },
  "homepage": "https://github.com/jmmarotta/agilt/tree/main/packages/cli#readme"
}
