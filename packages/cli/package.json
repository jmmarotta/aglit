{
  "name": "aglit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "aglit": "./dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm --sourcemap --external @aglit/sdk --external @stricli/core --banner '#!/usr/bin/env bun\n' && tsgo -p tsconfig.json --emitDeclarationOnly",
    "typecheck": "turbo run build --filter @aglit/sdk && tsgo -p tsconfig.test.json --noEmit",
    "lint": "oxlint src",
    "fmt": "oxfmt --write src",
    "fmt:check": "oxfmt --check src",
    "test": "turbo run build --filter @aglit/sdk && bun test"
  },
  "dependencies": {
    "@aglit/sdk": "workspace:*",
    "@stricli/core": "^1"
  }
}
