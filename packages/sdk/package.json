{
  "name": "@aglit/sdk",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm --sourcemap && tsgo -p tsconfig.json --emitDeclarationOnly",
    "typecheck": "tsgo -p tsconfig.test.json --noEmit",
    "lint": "oxlint src",
    "fmt": "oxfmt --write src",
    "fmt:check": "oxfmt --check src",
    "test": "bun test"
  },
  "dependencies": {}
}
